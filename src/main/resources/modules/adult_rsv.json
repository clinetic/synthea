{
  "name": "Adult RSV",
  "remarks": [
    "Module to simulate neonatal RSV for babys 2 months to 2 years old.",
    "",
    "Babys may receive the nirsevimab vaccine and decrease the probability they get RSV.",
    "",
    "Small chance of escalation of RSV to bronchitis, or mechanical ventilation. ",
    "",
    "Module will delay after a year to simulate seasonality chance of RSV and vaccination cycle. "
  ],
  "states": {
    "Initial": {
      "type": "Initial",
      "direct_transition": "age guard",
      "name": "Initial"
    },
    "Terminal": {
      "type": "Terminal",
      "name": "Terminal"
    },
    "RSV Infection": {
      "type": "ConditionOnset",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": 55735004,
          "display": "Respiratory syncytial virus infection",
          "value_set": ""
        }
      ],
      "name": "RSV Infection",
      "direct_transition": "RSV test"
    },
    "": {
      "type": "Simple",
      "distributed_transition": [
        {
          "distribution": 1
        }
      ],
      "name": ""
    },
    "delay season": {
      "type": "Delay",
      "distribution": {
        "kind": "EXACT",
        "parameters": {
          "value": 1
        }
      },
      "unit": "years",
      "name": "delay season",
      "direct_transition": "Next Season"
    },
    "Vaccinnation chance": {
      "type": "Simple",
      "name": "Vaccinnation chance",
      "distributed_transition": [
        {
          "transition": "RSV vaccine count",
          "distribution": 0.5
        },
        {
          "transition": "Non Vaccinated RSV Infection chance",
          "distribution": 0.5
        }
      ]
    },
    "Vaccinated RSV Chance": {
      "type": "Simple",
      "name": "Vaccinated RSV Chance",
      "distributed_transition": [
        {
          "transition": "RSV Infection",
          "distribution": 0.05
        },
        {
          "transition": "delay season",
          "distribution": 0.95
        }
      ]
    },
    "Non Vaccinated RSV Infection chance": {
      "type": "Simple",
      "name": "Non Vaccinated RSV Infection chance",
      "distributed_transition": [
        {
          "transition": "delay season",
          "distribution": 0.2
        },
        {
          "transition": "RSV Infection",
          "distribution": 0.8
        }
      ]
    },
    "RSV Encounter (escalation)": {
      "type": "Encounter",
      "encounter_class": "emergency",
      "reason": "",
      "telemedicine_possibility": "none",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": 79479005,
          "display": "Respiratory syncytial virus bronchitis",
          "value_set": ""
        }
      ],
      "name": "RSV Encounter (escalation)",
      "distributed_transition": [
        {
          "transition": "Mechanical Ventilation",
          "distribution": 0.1
        },
        {
          "transition": "End RSV condition",
          "distribution": 0.9
        }
      ]
    },
    "End RSV condition": {
      "type": "ConditionEnd",
      "direct_transition": "delay season",
      "name": "End RSV condition"
    },
    "Mechanical Ventilation": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": 243141005,
          "display": "Mechanically assisted spontaneous ventilation (procedure)",
          "value_set": ""
        }
      ],
      "distribution": {
        "kind": "UNIFORM",
        "parameters": {
          "high": 7,
          "low": 1
        }
      },
      "unit": "days",
      "distributed_transition": [
        {
          "transition": "End RSV condition",
          "distribution": 0.9
        },
        {
          "transition": "RSV Death",
          "distribution": 0.1
        }
      ],
      "name": "Mechanical Ventilation",
      "reason": "RSV Infection"
    },
    "RSV Season": {
      "type": "Guard",
      "allow": {
        "condition_type": "Date",
        "operator": ">",
        "month": 9
      },
      "direct_transition": "Vaccinnation chance",
      "name": "RSV Season"
    },
    "age guard": {
      "type": "Guard",
      "allow": {
        "condition_type": "Age",
        "operator": "<=",
        "quantity": 60,
        "unit": "years",
        "value": 0
      },
      "direct_transition": "RSV Season",
      "name": "age guard"
    },
    "RSV vaccine count": {
      "type": "Counter",
      "attribute": "RSV Adult Vaccination",
      "action": "increment",
      "name": "RSV vaccine count",
      "distributed_transition": [
        {
          "transition": "Get Abrysvo Vaccination",
          "distribution": 0.5
        },
        {
          "transition": "Get Arexvy Vaccination",
          "distribution": 0.5
        }
      ]
    },
    "Next Season": {
      "type": "Guard",
      "allow": {
        "condition_type": "Date",
        "operator": ">",
        "month": 9
      },
      "name": "Next Season",
      "direct_transition": "Vaccine Check"
    },
    "Vaccine Check": {
      "type": "Simple",
      "name": "Vaccine Check",
      "conditional_transition": [
        {
          "transition": "Vaccinated RSV Chance",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "RSV AdultVaccination",
            "operator": "==",
            "value": 1
          }
        },
        {
          "transition": "Non Vaccinated RSV Infection chance"
        }
      ]
    },
    "RSV Death": {
      "type": "Death",
      "name": "RSV Death",
      "direct_transition": "Terminal"
    },
    "Get Arexvy Vaccination": {
      "type": "Vaccine",
      "series": 1,
      "codes": [
        {
          "system": "CVX",
          "code": 303,
          "display": "RSV, recombinant, protein subunit RSVpreF, adjuvant reconstituted, 0.5 mL, PF",
          "value_set": ""
        }
      ],
      "direct_transition": "Vaccinated RSV Chance",
      "name": "Get Arexvy Vaccination"
    },
    "Get Abrysvo Vaccination": {
      "type": "Vaccine",
      "series": 1,
      "codes": [
        {
          "system": "CVX",
          "code": 305,
          "display": "RSV, bivalent, protein subunit RSVpreF, diluent reconstituted, 0.5 mL, PF",
          "value_set": ""
        }
      ],
      "name": "Get Abrysvo Vaccination",
      "direct_transition": "Vaccinated RSV Chance"
    },
    "RSV test": {
      "type": "MultiObservation",
      "category": "laboratory",
      "number_of_observations": 0,
      "codes": [
        {
          "system": "LOINC",
          "code": "40988-8",
          "display": "Respiratory syncytial virus RNA [Presence] in Specimen by NAA with probe detec",
          "value_set": ""
        }
      ],
      "name": "RSV test",
      "observations": [
        {
          "category": "laboratory",
          "unit": "string",
          "codes": [
            {
              "system": "LOINC",
              "code": "40988-8",
              "display": "Respiratory syncytial virus RNA [Presence] in Specimen by NAA with probe detection"
            }
          ],
          "exact": {
            "quantity": "positive"
          }
        },
        {
          "category": "laboratory",
          "unit": "string",
          "codes": [
            {
              "system": "LOINC",
              "code": "95422-2",
              "display": "Influenza virus A and B and SARS-CoV-2 (COVID-19) RNA panel - Respiratory system specimen by NAA with probe detection"
            }
          ],
          "exact": {
            "quantity": "positive"
          }
        }
      ],
      "distributed_transition": [
        {
          "transition": "RSV Encounter (escalation)",
          "distribution": 0.1
        },
        {
          "transition": "End RSV condition",
          "distribution": 0.9
        }
      ]
    }
  },
  "gmf_version": 2
}